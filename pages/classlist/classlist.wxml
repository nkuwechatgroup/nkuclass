<!--pages/classlist/classlist.wxml-->
<template name = "classes2" >
    <view class='one' wx:if="{{count % 2 ==0 & num== '0000'}}">-</view>
    <view class='full' wx:elif="{{count % 2 ==1 & num== '0000'}}" >-</view>
    <view class='one' wx:elif="{{count % 2 ==0 & num== '3333'}}">✓</view>
    <view class='full' wx:elif="{{count % 2 ==1 & num== '3333'}}" >✓</view>
    <view class='one' wx:elif="{{count % 2 ==0 & num== '5000'}}">x</view>
    <view class='full' wx:elif="{{count % 2 ==1 & num== '5000'}}" >x</view>
    <view class='one' wx:elif="{{count % 2 ==0 & num!= '0000'}}">{{num}}</view>
    <view class='full' wx:elif="{{count % 2 ==1 & num!= '0000'}}" >{{num}}</view>
</template>


